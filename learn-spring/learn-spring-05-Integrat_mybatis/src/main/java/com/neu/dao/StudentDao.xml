<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace：命名空间，唯一，dao接口全路径名称 -->
<mapper namespace="com.neu.dao.StudentDao">
    <!-- select查询、update更新、insert插入、delete删除 -->

    <select id="selectStudents" resultType="com.neu.domain.Student">
        select id,name,age,phone from student order by id
    </select>

    <insert id="insertStudents">
        insert into student values (#{id},#{name},#{age},#{phone})
    </insert>


</mapper>